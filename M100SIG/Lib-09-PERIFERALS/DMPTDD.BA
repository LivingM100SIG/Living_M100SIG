10 '"DMPTDD.BA" -- Program to Read TDD       Sectors using P-DOS
20 CLS:LFILESV:CLEAR256,MAXRAM-1292:F$="DMPOUT":OPENF$FOROUTPUTAS1
30 GOSUB80:FORI=ATOB:CLS:PRINT@87,"Now reading Sector No."I;
40 DSKO$0,I,HIMEM
50 PRINT#1,"---------":PRINT#1,"Sector"I:PRINT#1,"---------":FORJ=0TO1291:PRINT#1,CHR$(PEEK(HIMEM+J));:NEXT
60 PRINT#1,:BEEP:NEXT
70 CLOSE:CLEAR266,MAXRAM:MENU
80 PRINT:PRINT"TDD Sector Dumper For Use with POWR-DOS":PRINT:INPUT"Starting Sector (0-79)";A:INPUT"Ending Sector   (0-79)";B:RETURN
