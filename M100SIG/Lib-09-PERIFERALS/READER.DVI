0 'ASCII File Reader V1
5  LPRINTCHR$(27);"0":LPRINTCHR$(15)
10 CLS:MAXFILES=2:CLEAR256
20 INPUT"Name of Disk file ";N$
25 OPEN "0:"+N$ FOR INPUT AS 1
27 INPUT"Hardcopy ";K$
30 IF K$<>"n"ANDK$<>"N"THEN GOSUB80
40 IF EOF(1) THENPRINT : PRINT "End of File":CLOSE:END
50 LINE INPUT#1, A$
60 PRINT A$:IF F=1THEN PRINT#2,A$
70 GOTO40
80 OPEN"lpt:"FOR OUTPUT AS 2:F=1:RETURN
