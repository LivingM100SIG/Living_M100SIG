10 PRINT, "CHI SQUARE PROGRAM FOR"
20 PRINT, "TWO-BY-TWO TABLE"
30 INPUT "INPUT ROW 1 COL 1 ";A
40 INPUT "INPUT ROW 1 COL 2 ";B
50 INPUT"INPUT ROW 2 COL 1 ";C
60 INPUT"INPUT ROW 2 COL 2 ";D
70 LETX=((A*D)-(B*C))^2
80 LETY=A+B+C+D
90 LET Z=(A+B)*(C+D)*(B+D)*(A+C)
100 CHI=(X*Y)/Z
102 CHI=CHI+.0005
104 CHI=INT(CHI*1000)/1000
110 CLS:PRINT:PRINT
120 PRINT,"CHI SQUARE=";CHI
125 L=((A*D)-(B*C))^2
130 V=SQR(L)
135 W=(V-(A+B+C+D)/2)^2
140 Q=W*Y/Z
145 Q=Q+.0005:Q=INT(Q*1000)/1000
150 PRINT"(WITH YATE'S CORRECTION= ";Q;")"
160 IF CHI<3.841THENPRINT"P>.O5"
170 IF CHI>3.841ANDCHI<5.024THENPRINT"P IS BETWEEN .05 AND .025"
175 IFCHI>5.024ANDCHI<6.635THENPRINT"P IS BETWEEN .025 AND .01"
180 IF CHI>6.635ANDCHI<7.879THENPRINT"P IS BETWEEN .01 AND .005"
185 IFCHI>7.879ANDCHI<10.82THENPRINT"P IS BETWEEN .005 AND .001"
190 IF CHI>10.828THEN PRINT "P IS LESS THAN 0.001"
200 PRINT"HIT <ENTER> TO CONTINUE":PRINT"HIT ANY OTHER KEY FOR MENU";:A$=INPUT$(1):IFA$<>CHR$(13)THENMENU
210 CLS:GOTO10

