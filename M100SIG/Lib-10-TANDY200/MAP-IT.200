40 X=X1:Y=Y1:INPUT#1,X1,Y1
50 LINE(X1+10,Y1-10)-(X+10,Y-10)
60 IFX1=SANDY1=TTHEN20
70 GOTO 40
80 PRINT@560,"State:";:LINEINPUT"";X$
90 CLS:CLOSE:OPEN"STADAT"FORINPUT AS 1
100 IF EOF(1) THEN 160
110 INPUT#1,S$
120 IFS$<>X$THENLINEINPUT#1,S$:GOTO100
130 LINEINPUT#1,S$:IFS$=X$THEN150
140 PRINTS$:GOTO130
150 LINEINPUT"";X$
160 BEEP:RUN
