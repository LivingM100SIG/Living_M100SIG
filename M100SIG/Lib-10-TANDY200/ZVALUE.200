5 REM FRED BROWN [73267,2450]
10 CLS:CLEAR99:PRINT"ZVALUE.BA"
15 PRINT@120,"INPUT Z-VALUE:"
20 INPUTZ:PRINT@280,"Working..."
25 D=SGN(Z):Z=ABS(Z):Z2=Z*Z
30 A=Z*1/SQR(8*ATN(1)):B=A:P=1
35 X=X+1:P=-P*Z2/2/X:P1=P/(2*X+1)*B
