10 REM DESIGN George Saladino 70210,445
15 REM Adapted from a program in the first issue of Cload Magazine.
20 DEFINT A-Z
30 SEC=VAL(RIGHT$(TIME$,2))
40 FOR I=1 TO SEC
50 DUMMY=RND(1)
60 NEXT I
100 CLS
110 N=INT(RND(1)*50)
120 CLS:FOR R=1 TO N
130 X=INT(RND(1)*240)
140 Y=INT(RND(1)*64)
150 FOR I=20 TO 20+INT(RND(1)*40)
160 PSET(X,Y):PSET(239-X,Y):PSET(X,63-Y):PSET(239-X,63-Y)
170 IF X<239 THEN X=X+1
180 IF Y<63 THEN Y=Y+1
190 NEXT I:NEXTR
200 FOR T=1 TO 5000:NEXT
210 RUN

